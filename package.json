{
  "name": "use-structure",
  "version": "0.1.3",
  "description": "Hooks for working with basic JavaScript data structures",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --watch",
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.txt\" --source-maps inline",
    "prepare": "npm run build",
    "ts": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/baron816/use-structure.git"
  },
  "keywords": [
    "React",
    "hooks",
    "proxy",
    "array",
    "object",
    "set",
    "map"
  ],
  "author": "Baron Willeford",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.0 || 17.0.0"
  },
  "dependencies": {
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.16",
    "@types/react": "^16.9.46",
    "babel-jest": "^26.6.3",
    "eslint": "^7.18.0",
    "eslint-config-baron": "^1.0.0",
    "eslint-config-prettier": "^7.1.0",
    "jest": "26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  }
}
