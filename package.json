{
  "name": "use-structure",
  "version": "0.1.0",
  "description": "Hooks for working with basic JavaScript data structures",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --watch",
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.txt\" --source-maps inline",
    "prepare": "npm run build",
    "ts": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/baron816/use-structure.git"
  },
  "keywords": [
    "React",
    "hooks",
    "proxy",
    "array",
    "object",
    "set",
    "map"
  ],
  "author": "Baron Willeford",
  "license": "MIT",
  "dependencies": {
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.9",
    "@types/react": "^16.9.46",
    "babel-jest": "^26.3.0",
    "jes": "^0.6.1",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.7"
  }
}
